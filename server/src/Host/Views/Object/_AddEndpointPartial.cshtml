@using MySql.AspNet.Identity;
@model ObjectEndpoint

<form asp-controlle="Object" asp-action="_AddEndpointPartial" method="post" class="form-inline">
    <div asp-validation-summary="All" class="text-danger"></div>
    <input type="hidden" asp-for="Id" />
    <input type="hidden" asp-for="ObjectId" />
    <input class="form-control" asp-for="Value" style="width:20%" />
    <input class="form-control" asp-for="Description" style="width:20%" />
    <button type="button" class="btn btn-success"><span class="glyphicon glyphicon-ok"></span></button>
    <button type="button" class="btn btn-default"><span class="glyphicon glyphicon-ban-circle"></span></button>
    <span asp-validation-for="Value" class="text-danger"></span>
</form>